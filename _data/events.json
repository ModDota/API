{
    "AddStyle": {
        "description": "Add a CSS class to a panel.",
        "panelEvent": true,
        "args": [
            {
                "name": "class",
                "type": "string"
            }
        ]
    },
    "AddStyleAfterDelay": {
        "description": "Add a CSS class to a panel after a specified delay. ",
        "panelEvent": true,
        "args": [
            {
                "name": "class",
                "type": "string"
            },
            {
                "name": "pre-delay",
                "type": "float"
            }
        ]
    },
    "AddStyleToEachChild": {
        "description": "Add a CSS class to all children of this panel.",
        "panelEvent": true,
        "args": [
            {
                "name": "class",
                "type": "string"
            }
        ]
    },
    "AddTimedStyle": {
        "description": "Add a class for a specified duration, with optional pre-delay; clears existing timers when called with same class. ",
        "panelEvent": true,
        "args": [
            {
                "name": "class",
                "type": "string"
            },
            {
                "name": "duration",
                "type": "float"
            },
            {
                "name": "pre-delay",
                "type": "float"
            }
        ]
    },
    "AsyncEvent": {
        "description": "Fire another event after a delay (in seconds).",
        "panelEvent": false,
        "args": [
            {
                "name": "delay",
                "type": "float"
            },
            {
                "name": "eventToFire",
                "type": "event"
            }
        ]
    },
